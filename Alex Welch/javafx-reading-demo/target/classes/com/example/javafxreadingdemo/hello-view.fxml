<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="15.0" fillWidth="false"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxreadingdemo.HelloController">
    <TextField text="TextField"/>
    <Label text="Label" fx:id="welcomeText"/>
    <HBox spacing="15.0" alignment="CENTER">
        <Button text="Button 1" onAction="#onButton1Click"/>
        <Button text="Button 2" onAction="#onButton2Click"/>
        <Button fx:id="confirmBtn" text="Button 3" onAction="#onButton3Click"/>
    </HBox>
</VBox>


